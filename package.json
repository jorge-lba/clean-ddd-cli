{
  "name": "clean-ddd-cli",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "license": "MIT",
  "bin": {
    "clean-ddd-cli": "./dist/index.js"
  },
  "dependencies": {
    "string-replace-stream": "^0.0.2",
    "yargs": "^17.6.2"
  },
  "scripts": {
    "run:cli": "tsx src/index.ts",
    "build": "tsc -p ./tsconfig.build.json",
    "test": "vitest",
    "test:stage": "vitest $(git ls-files --other --modified --exclude-standard | grep \".spec.ts\")",
