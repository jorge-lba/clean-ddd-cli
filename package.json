    "@typescript-eslint/parser": "^5.56.0",
    "@vitest/ui": "^0.27.2",
    "docsify-cli": "^4.4.4",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "tsup": "^6.5.0",
    "tsx": "^3.12.2",
    "vitest": "^0.27.2"
  },
  "files": [
    "dist",
    "src/base"
